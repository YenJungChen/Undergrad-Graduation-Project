<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/gg" >

	<RelativeLayout
	    android:layout_width="915px"
	    android:layout_height="919px"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/tw"
	    android:focusable="true"
	    android:focusableInTouchMode="true" >

	    

	    <ImageButton
	        android:id="@+id/tutor"
	        android:layout_width="301px"
	        android:layout_height="303px"
	        android:layout_toLeftOf="@+id/view"
	        android:layout_marginRight="-180px"
	        android:layout_below="@+id/play"
	        android:layout_marginTop="90px"
	        android:background="@drawable/t" />

	    <ImageButton
	        android:id="@+id/about"
	        android:layout_width="258px"
	        android:layout_height="260px"
	        android:layout_below="@+id/user"
	        android:layout_marginTop="67px"	        
	        android:layout_toRightOf="@+id/view"
	        android:layout_marginLeft="-112px"
	        android:background="@drawable/a"/>

	    <ImageButton
	        android:id="@+id/user"
	        android:layout_width="295px"
	        android:layout_height="297px"
	        android:layout_toRightOf="@+id/view"
	        android:layout_marginLeft="-125px"
	        android:layout_marginTop="110px"
	        android:background="@drawable/u" />

	    <ImageButton
	        android:id="@+id/play"
	        android:layout_width="280px"
	        android:layout_height="283px"	        
	        android:layout_toLeftOf="@+id/view"
	        android:layout_marginRight="-135px"
	        android:layout_marginTop="127px"
	        android:background="@drawable/d" />
	    <ImageButton
	        android:id="@+id/view"
	        android:layout_width="372px"
	        android:layout_height="375px"
	        android:layout_centerHorizontal="true"
	        android:layout_centerVertical="true"
	        android:background="@drawable/s" />

	</RelativeLayout>

</RelativeLayout>
